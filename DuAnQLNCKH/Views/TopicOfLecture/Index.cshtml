@model IEnumerable<DuAnQLNCKH.Models.TopicOfLecture>

@{
    ViewBag.Title = "Danh sách đề tài";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var a = (List<DuAnQLNCKH.Models.TopicOfStudent>)ViewBag.TopicOfStudents;
}
<style>
    table td, th {
        padding: 8px;
    }

    table tr td {
        margin-left: 20px;
    }
</style>

<div class="tabs">
    <div class="tab-nav-wrapper">
        <ul>
            <li><a href="#tab1">Đề tài giảng viên</a></li>
            <li><a href="#tab2">Đề tài sinh viên</a></li>

        </ul>
    </div>
    <div class="tab-content-wrapper" style="width: 1200px">
        <div id="tab1" class="tab-content" style="display:none;">
            <table style="text-align:center">
                <tr>
                    <th>
                        Mã đề tài
                    </th>
                    <th style="width:100px">
                        Tên đề tài
                    </th>
                    <th>
                        Mã giảng viên
                    </th>
                    <th>
                        Mã điểm
                    </th>
                    <th>
                        Bắt đầu
                    </th>
                    <th>
                        Thời gian
                    </th>
                    <th>
                        Kinh phí
                    </th>
                    <th>
                        Trạng Thái
                    </th>
                    <th>
                        Progress
                    </th>

                </tr>
                @foreach (DuAnQLNCKH.Models.TopicOfLecture deTaiGV in Model)
                {

                    <tr>
                        <td>

                            @deTaiGV.IdTp


                        </td>
                        <td style="width:100px">

                            @deTaiGV.Name

                        </td>
                        <td>

                            @deTaiGV.IdLe


                        </td>
                        <td>

                            @deTaiGV.IdP


                        </td>
                        <td>

                            @deTaiGV.DateSt

                        </td>
                        <td>

                            @deTaiGV.Times

                        </td>
                        <td>

                            @deTaiGV.Expense

                        </td>
                        <td>

                            @deTaiGV.Status

                        </td>
                        <td>

                            @deTaiGV.Progress

                        </td>



                    </tr>

                }



            </table>
        </div>
        <div id="tab2" class="tab-content" style="display:none;">
            <table width="1000px" style="text-align:center">
                <tr>
                    <th>
                        Mã đề tài
                    </th>
                    <th>
                        Tên đề tài
                    </th>
                    <th>
                        Tên sinh viên
                    </th>
                    <th>
                        Mã sinh viên
                    </th>
                    <th>
                        Email
                    </th>
                    <th>
                        Mã điểm
                    </th>
                    <th>
                        Bắt đầu
                    </th>
                    <th>
                        Thời gian
                    </th>
                    <th>
                        Kinh phí
                    </th>
                    <th>
                        Trạng Thái
                    </th>
                    <th>
                        Progress
                    </th>

                </tr>

                @foreach (var topicOfStudent in a)
                {

                    <tr>
                        <td>

                            @topicOfStudent.IdP


                        </td>
                        <td style="width:100px">

                            @topicOfStudent.Name

                        </td>
                        <td>

                            @topicOfStudent.NameSt


                        </td>
                        <td>

                            @topicOfStudent.IdSV


                        </td>
                        <td>

                            @topicOfStudent.Emmail

                        </td>
                        <td>

                            @topicOfStudent.IdP

                        </td>
                        <td>

                            @topicOfStudent.DateSt

                        </td>
                        <td>

                            @topicOfStudent.Times

                        </td>
                        <td>

                            @topicOfStudent.Expense

                        </td>
                        <td>

                            @topicOfStudent.Status

                        </td>
                        <td>

                            @topicOfStudent.Progress

                        </td>

                    </tr>

                }



            </table>
        </div>

    </div>
</div>

